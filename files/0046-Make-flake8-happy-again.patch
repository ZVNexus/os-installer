From dbcce2f2165bb51a67661dc01befdf1ca663fc09 Mon Sep 17 00:00:00 2001
From: Ikey Doherty <ikey@solus-project.com>
Date: Sun, 12 Jun 2016 12:02:14 +0100
Subject: [PATCH 46/51] Make flake8 happy again

Signed-off-by: Ikey Doherty <ikey@solus-project.com>
---
 os_installer2/pages/progress.py | 2 +-
 os_installer2/pages/welcome.py  | 8 ++++----
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/os_installer2/pages/progress.py b/os_installer2/pages/progress.py
index 9d7e0b9..1a887c1 100644
--- a/os_installer2/pages/progress.py
+++ b/os_installer2/pages/progress.py
@@ -185,7 +185,7 @@ class InstallerProgressPage(BasePage):
                   "hasn't completed. The following errors were encountered: "
         else:
             msg = "Installation has failed, and changes were made to disk\n" \
-                   "The installer will now exit."
+                  "The installer will now exit."
 
         msg += "\n\n{}\n".format("\n".join(self.error_msgs))
         d = Gtk.MessageDialog(parent=self.info.owner,
diff --git a/os_installer2/pages/welcome.py b/os_installer2/pages/welcome.py
index 150e762..cd0efee 100644
--- a/os_installer2/pages/welcome.py
+++ b/os_installer2/pages/welcome.py
@@ -45,8 +45,8 @@ class InstallerWelcomePage(BasePage):
         self.grid.attach(button, 0, 1, 1, 1)
         # Install label
         label = Gtk.Label.new("<big>{}</big>\n<small>{}</small>".format(
-          "Install Solus to disk",
-          "Permanently use Solus on your device"))
+                              "Install Solus to disk",
+                              "Permanently use Solus on your device"))
         label.get_style_context().add_class("dim-label")
         label.set_use_markup(True)
         self.grid.attach(label, 0, 2, 1, 1)
@@ -61,8 +61,8 @@ class InstallerWelcomePage(BasePage):
         self.grid.attach(button, 1, 1, 1, 1)
         # Continue label
         label = Gtk.Label.new("<big>{}</big>\n<small>{}</small>".format(
-          "Continue using live preview",
-          "No changes will be made to your system"))
+                              "Continue using live preview",
+                              "No changes will be made to your system"))
         label.get_style_context().add_class("dim-label")
         label.set_use_markup(True)
         self.grid.attach(label, 1, 2, 1, 1)
-- 
